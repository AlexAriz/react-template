{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "jsx": true
    },
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "indent": [
      "error",
      2
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      "error",
      "always"
    ],
    "no-dupe-args": "error",
    "no-dupe-keys": "error",
    "no-duplicate-case": "error",
    "no-extra-boolean-cast": "error",
    "no-unexpected-multiline": "error",
    "curly": "error",
    "default-case": "error",
    "dot-location": [
      "error",
      "property"
    ],
    "eqeqeq": [
      "error",
      "always"
    ],
    "no-alert": "error",
    "no-else-return": "error",
    "no-magic-numbers": "error",
    "no-multi-spaces": "error",
    "no-redeclare": "error",
    "yoda": [
      "error",
      "never",
      {
        "exceptRange": true
      }
    ],
    "camelcase": "error",
    "eol-last": [
      "error",
      "always"
    ],
    "func-call-spacing": "error",
    "key-spacing": [
      "error",
      {
        "beforeColon": false,
        "afterColon": true
      }
    ],
    "max-len": [
      "error",
      120,
      2
    ],
    "max-params": [
      "error",
      5
    ],
    "newline-before-return": "error",
    "no-tabs": "error",
    "no-multiple-empty-lines": [
      "error",
      {
        "max": 1,
        "maxEOF": 1
      }
    ],
    "no-trailing-spaces": "error",
    "one-var-declaration-per-line": [
      "error",
      "always"
    ],
    "no-confusing-arrow": [
      "error",
      {
        "allowParens": true
      }
    ],
    "no-const-assign": "error",
    "no-useless-computed-key": "error",
    "no-useless-rename": "error",
    "no-var": "error",
    "prefer-const": "error",
    "rest-spread-spacing": "error",
    "no-unused-vars": [
      "error",
      {
        "varsIgnorePattern": "React"
      }
    ]
  }
}